                        <h6 class="mb-3 text-sm font-medium text-gray-900 dark:text-white">
                            orgin
                        </h6>
                        <ul class="space-y-2 text-sm" aria-labelledby="dropdownDefault">
                            <li class="flex items-center">
                                <input id="apple" type="checkbox" value=""
                                       class="w-4 h-4 bg-gray-100 border-gray-300 rounded text-primary-600 focus:ring-primary-500 dark:focus:ring-primary-600 dark:ring-offset-gray-700 focus:ring-2 dark:bg-gray-600 dark:border-gray-500" />
                                <label for="apple" class="ml-2 text-sm font-medium text-gray-900 dark:text-gray-100">
                                    Germany (56)
                                </label>
                            </li>

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="border-2 border-solid rounded-lg border-gray-100 dark:border-gray-600 h-full mb-4 p-3">
        {% block actor_list_frame %}
            <turbo-frame id="actor-list" data-turbo-action="advance">
        <div
                class="relative overflow-x-auto shadow-md sm:rounded-lg"
                data-controller="checkbox-select-all check"
        >
            <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
                <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
                <tr>
                    <th scope="col" class="p-4">
                        <div class="flex items-center">
                            <input id="checkbox-all-search" type="checkbox" class="w-4 h-4 text-blue-600 bg-gray-100 border-gray-300 rounded focus:ring-blue-500 dark:focus:ring-blue-600 dark:ring-offset-gray-800 dark:focus:ring-offset-gray-800 focus:ring-2 dark:bg-gray-700 dark:border-gray-600">
                            <label for="checkbox-all-search" class="sr-only">checkbox</label>
                        </div>
                    </th>
                    <th scope="col" class="px-6 py-3">
                        <div class="flex items-center">
                            <a href="{{ path('app_actor_index', {
                                ...app.request.query.all(),
                                sort: 'name',
                                sortDirection: sort == 'name' and sortDirection == 'asc' ? 'desc' : 'asc',
                            }) }}">
                                Name {{ colheader.sortArrow('name', sort, sortDirection) }}
                            </a>
                        </div>
                    </th>
                    <th scope="col" class="px-6 py-3">
                        <div class="flex items-center">
                            <a href="{{ path('app_actor_index', {
                                ...app.request.query.all(),
                                sort: 'orgin',
                                sortDirection: sort == 'orgin' and sortDirection == 'asc' ? 'desc' : 'asc',
                            }) }}">
                                Orgin {{ colheader.sortArrow('orgin', sort, sortDirection) }}
                            </a>
                        </div>
                    </th>
                    <th scope="col" class="px-6 py-3">
                        <div class="flex items-center">
                            <a href="{{ path('app_actor_index', {
                                ...app.request.query.all(),
                                sort: 'bornAt',
                                sortDirection: sort == 'bornAt' and sortDirection == 'asc' ? 'desc' : 'asc',
                            }) }}">
                                bornAt {{ colheader.sortArrow('bornAt', sort, sortDirection) }}
                            </a>
                        </div>
                    </th>
                    <th scope="col" class="px-6 py-3">
                        <div class="flex items-center">
                            Status
                            <a href="#">
                                <svg class="w-3 h-3 ms-1.5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 24 24">
                                    <path d="M8.574 11.024h6.852a2.075 2.075 0 0 0 1.847-1.086 1.9 1.9 0 0 0-.11-1.986L13.736 2.9a2.122 2.122 0 0 0-3.472 0L6.837 7.952a1.9 1.9 0 0 0-.11 1.986 2.074 2.074 0 0 0 1.847 1.086Zm6.852 1.952H8.574a2.072 2.072 0 0 0-1.847 1.087 1.9 1.9 0 0 0 .11 1.985l3.426 5.05a2.123 2.123 0 0 0 3.472 0l3.427-5.05a1.9 1.9 0 0 0 .11-1.985 2.074 2.074 0 0 0-1.846-1.087Z"></path>
                                </svg>
                            </a>
                        </div>
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Action
                    </th>
                </tr>
                </thead>
                <tbody>
                {% for item in pager %}
                    {{ include('actor/_row.html.twig') }}
                {% endfor %}
                </tbody>
            </table>
            <div class="h-2.5 md:h-auto flex justify-end m-2.5">
                <div class="flex items-center mt-6 space-x-4">
                    {% if pager.hasPreviousPage %}
                        <a href="{{ pagerfanta_page_url(pager, pager.previousPage) }}"
                           class="block py-2 px-4 bg-gray-700 text-white rounded hover:bg-gray-600">Previous</a>
                    {% endif %}
                    {% if pager.hasNextPage %}
                        <a href="{{ pagerfanta_page_url(pager, pager.nextPage) }}"
                           class="block py-2 px-4 bg-gray-700 text-white rounded hover:bg-gray-600">Next</a>
                    {% endif %}
                    <div class="ml-4">
                        Page {{ pager.currentPage }}/{{ pager.nbPages }}
                    </div>
                </div>
            </div>
        </div>
        </turbo-frame>
